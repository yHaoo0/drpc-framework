<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm} [%t] | %-5level %-25logger{1} | %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="coding.dreamlash.drpcframework" level="info" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Root level="warn">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>